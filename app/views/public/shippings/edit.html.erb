<div class="row">
    <div class="col-xs-12">
        <h5><span class="heading">配送先編集</span></h5> 

        <%= form_with model: (:publi @shippings) local: true ,method: :patch do |f| %>
        
                <div class="shippinges col-xs-10">
                <div class="form-group col-xs-12">
                    <%= f.label :"郵便番号(ハイフンなし)" %>
                    <%= f.text_field :post_code %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"住所" %>
                    <%= f.text_field :address %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"宛名" %>
                    <%= f.text_field :name %>
                </div>
                <div class="form-group col-xs-12">
                    <%= f.submit '編集する' %>
                </div>
            </div>
        <% end %>
    </div>
</div>
            <div class="row">
                <div class="col-xs-12">
                    <%= link_to "配送先登録/一覧へ戻る", public_shippings_path %>
                </div>
            </div>
            
           