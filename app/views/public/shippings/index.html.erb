
<div class="row">
     <div class="col-xs-12">
     <h5><span class="heading">配送先登録/一覧へ戻る</span></h5>
        
           <%= form_with model:[:public, @shipping], local:true do |f| %>
           <div class="shippings col-xs-10">
               <div class="form-group col-xs-12">
                    <%= f.label :"郵便番号(ハイフンなし)" %>
                    <%= f.text_field :post_code, class: "col-xs-4 form-control-static post_code" %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"住所" %>
                    <%= f.text_field :address, class: "col-xs-7 form-control-static address" %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"宛名" %>
                    <%= f.text_field :address_name, class: "col-xs-4 form-control-static address_name" %>
                </div>
                <%= f.submit '登録する' ,class: "col-xs-2 pull-right btn-sm btn-success " %>
             </div>
           <% end %>
           </div>
     </div>
</div>

        <div class="row">
         <div id="create">
          <%= render "ship", shippings: @shippings %>
         </div>
        </div>