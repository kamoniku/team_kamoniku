<div class="row">
    
        <div id="create">
          <%= render "ship", shippings: @shippings %>
        </div>
        
     <div class="col-xs-12">
        <h2 class="text-center">配送先登録</h2>
        
        <%= form_with model:@shipping, local:true do |f| %>
               <div class="form-group col-xs-12">
                    <%= f.label :"郵便番号(ハイフンなし)" %>
                    <%= f.text_field :post_code %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"住所" %>
                    <%= f.text_field :address %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"宛名" %>
                    <%= f.text_field :address_name %>
                </div>
                <%= f.submit '登録する' ,class: " btn-success " %>
        <% end %>
     </div>
</div>