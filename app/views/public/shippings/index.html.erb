<div class="row">
    
        <div id="create">
          <%= render "ship", shippings: @shippings %>
        </div>
        
     <div class="col-xs-12">
        <h2 class="text-center">配送先登録/一覧</h2>
        
        <!--%= form_with(model: @shippings, :url => {:controller => "customer/shippings"}) do |f| %>-->
        <%= form_with(model:[public, @shippings], local: true) do |f| %>
                <div class="form-group col-xs-12">
                    <%= f.label :"郵便番号(ハイフンなし)" %>
                    <%= f.text_field :post_code %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"住所" %>
                    <%= f.text_field :address %>
                </div>

                <div class="form-group col-xs-12">
                    <%= f.label :"宛名" %>
                    <%= f.text_field :name %>
                </div>
                <%= f.submit '登録する' ,class: " btn-success " %>
        <% end %>
     </div>
</div>